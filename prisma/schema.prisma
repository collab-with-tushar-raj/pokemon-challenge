// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pokemon {
  id    Int     @id @default(autoincrement())
  pokemonId Int
  name String
  types  String
  attack Int
  defense Int
  hp Int
  speed Int
  specialAttack Int 
  specialDefense Int
}